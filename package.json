{
  "name": "om-component-lib-playground",
  "version": "0.1.0",
  "description": "a playground for experimenting with a component library",
  "main": "server.js",
  "scripts": {
    "dev:install": "npm install && npm run symlink",
    "symlink": "./scripts/symlink.sh",
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --hot --inline",
    "build:watch": "webpack --config webpack.config.js && webpack-dev-server --hot --inline",
    "build:prod:watch": "webpack --config webpack.prod.config.js && webpack-dev-server --inline",
    "dev:debug": "webpack-dev-server --hot --inline --debug",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.spec.js --require ignore-styles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@westfield/om-react-components": "~0.1.4",
    "@westfield/one-base": "~0.1.3",
    "@westfield/one-styles": "~0.1.3",
    "@westfield/one-utilities": "~0.1.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "file-loader": "^1.1.6",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-modal": "^3.1.11",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "repository": "https://github.com/meechanism/om-component-lib-playground"
}
